	<queryString>
		<![CDATA[SELECT CP.*, RCP.*, PAR.PAR_NOME_RAZAO_SOCIAL,
COALESCE(rcp_valor_pago,0) AS rcp_valor_pago11,
CASE  WHEN CP.CP_VALOR_DOCUMENTO = 
     (SELECT COALESCE(SUM(T.RCP_VALOR_PAGO),0) AS TOTAL 
      FROM RECEBIMENTO_CONTAS_PAGAR T WHERE T.RCP_CP_CODIGO_ID = CP.CP_CODIGO_ID) THEN RCP.RCP_VALOR_PAGO ELSE CP.CP_VALOR_DOCUMENTO END AS VALOR_TITULO
 FROM CONTAS_PAGAR CP
LEFT JOIN RECEBIMENTO_CONTAS_PAGAR RCP ON (RCP.rcp_cp_codigo_id = CP.CP_CODIGO_ID)
INNER JOIN PARCEIROS PAR ON (PAR.PAR_CODIGO_ID = CP.CP_PAR_CODIGO_ID)]]>
	</queryString>
